{
  "name": "web-sydney-rental-hub",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:tokens": "node scripts/build-tokens.js",
    "build:tokens:watch": "node scripts/build-tokens.js --watch",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:mini-program": "storybook dev -p 6007 -c packages/ui/.storybook",
    "test:visual": "loki test",
    "test:visual:approve": "loki approve",
    "test:visual:update": "loki update",
    "typecheck": "turbo run typecheck",
    "docs:dev": "pnpm --filter @srh/docs-site start",
    "docs:build": "pnpm --filter @srh/docs-site build",
    "docs:serve": "pnpm --filter @srh/docs-site serve",
    "astro:dev": "pnpm --filter @srh/design-site-astro dev",
    "astro:build": "pnpm --filter @srh/design-site-astro build",
    "astro:preview": "pnpm --filter @srh/design-site-astro preview"
  },
  "pnpm": {
    "overrides": {
      "storybook": "8.6.14",
      "@storybook/cli": "8.6.14",
      "@storybook/core-server": "8.6.14",
      "@storybook/vue3": "8.6.14",
      "@storybook/vue3-vite": "8.6.14",
      "@storybook/addon-essentials": "8.6.14",
      "@storybook/addon-interactions": "8.6.14",
      "@storybook/addon-links": "8.6.14",
      "@storybook/addon-a11y": "8.6.14",
      "@storybook/addon-docs": "8.6.14",
      "@storybook/addon-viewport": "8.6.14",
      "@storybook/blocks": "8.6.14",
      "@chromatic-com/storybook": "1.8.0",
      "vite": "^5.0.0",
      "vitest": "^1.0.0",
      "@vitest/browser": "^1.0.0",
      "@vitest/coverage-v8": "^1.0.0",
      "@vitest/ui": "^1.0.0"
    }
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@vitest/ui": "^1.0.0",
    "concurrently": "^9.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "loki": "^0.34.0",
    "rimraf": "^6.0.1",
    "style-dictionary": "^5.1.1",
    "turbo": "^2.5.8",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@astrouxds/tokens": "^1.14.0",
    "uni-app-x-mcp": "^0.0.5"
  }
}
