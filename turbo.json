{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:style": {
      "dependsOn": ["^lint:style"]
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    }
  }
}
