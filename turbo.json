{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "@web-sydney/backend#build": {
      "outputs": ["**/*.pyc"]
    },
    "@sydney-rental-hub/ui#build": {
      "dependsOn": ["sydney-rental-hub-token-builder#build"],
      "outputs": ["dist/**"]
    },
    "sydney-rental-hub-token-builder#build": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "typecheck": {
      "outputs": [],
      "dependsOn": ["^typecheck"]
    }
  }
}
